<main>
{{#with page_header}}
<h1 class="text-center">{{title}}</h1>
<p class="text-center text-uppercase">{{description}}</p>

{{/with}}      
{{#with section}}
  {{#with about_us}}
    {{#with title_list}}
    <div class="card">
      <div class="card-body">
    <p class="display-6 text-end">{{main}}</p>
    <h2 class="text-end">{{sub}}</h2>
      
    {{/with}}
    <br>
    <div><button class="btn btn-outline-info">{{button.text}}</button></div>
    </div>
    </div>
  {{/with}}
  <br>
  {{#with advantage}}
    <p class="display-5 text-muted">{{title}}</p>
    <br>
    {{#with efficiency}}
    <div class="card">
      <div class="card-body">
        <p class="display-4 text-muted">{{title}}</p>
        <h6>{{sub_title}}</h6>
        <br>
        <p>{{description}}</p>
        <br>
        <button class="btn btn-outline-warning">{{button}}</button>
      </div>
    </div>
    {{/with}}
    <br>
    {{#with innovation}}
      <div class="card">
      <div class="card-body">
        <p class="display-4 text-muted text-center">{{title}}</p>
        <h6 class="text-center">{{sub_title}}</h6>
        <p class="text-center">{{description}}</p>
        <button class="btn btn-outline-warning">{{button}}</button>
      </div>
    </div>
    {{/with}}
    <br>
    {{#with collaboration}}
      <div class="card">
      <div class="card-body">
        <p class="display-4 text-muted text-end">{{title}}</p>
        <h6 class="text-end">{{sub_title}}</h6>
        <p class="text-end">{{description}}</p>
        <button class="btn btn-outline-warning">{{button}}</button>
      </div>
    </div>
    {{/with}}
    <br>
    
    {{/with}}
    {{#with agreement}}
    <p class="display-5">{{title}}</p>
      {{#with info}}
      <div class="card">
        <div class="card-body">
          <h5>{{sub_title}}</h5>
          <br>
          {{#with detail}}
          <p>{{text}}</p>
          <br>
          <button class="btn btn-success">{{button}}</button>
          {{/with}}
        </div>
        </div>
      {{/with}}
    {{/with}}

{{/with}}
</main>