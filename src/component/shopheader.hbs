<section class='nav row p-0 mb-3'>
  {{#with navigation}}
  <div class='nav-item col-8'>
    <h1>{{Logo}}</h1>
  </div>
  <div class='nav col-4 text-end py-1 justify-content-end'>
    
      {{#each links}}
      <div class='nav-item'>
        <a class='nav-link pe-0 {{#if @last}} disabled {{/if}}' href={{href}}>{{text}}</a>
      </div>
      {{/each}}
  </div>
 
  {{/with}}
  <hr />
</section>